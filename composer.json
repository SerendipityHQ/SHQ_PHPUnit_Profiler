{
    "name": "serendipity_hq/phpunit_profiler",
    "license": "MIT",
    "type": "library",
    "description": "PHPUnit loggers to profile tests.",
    "keywords": ["phpunit", "profiling", "loggers"],
    "homepage": "https://github.com/SerendipityHQ/SHQ_PHPUnit_Profiler",
    "authors": [
        {
            "name": "Adamo crespi",
            "email": "hello@aerendir.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "SerendipityHQ\\Library\\PHPUnit_Profiler\\": "src/"
        }
    },
    "require": {
        "php": "^5.4|^7.0",
        "symfony/stopwatch": "*"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "dev-master",
        "fabpot/php-cs-fixer": "*",
        "phpunit/phpunit": "*",
        "satooshi/php-coveralls": "*@dev"
    }
}
